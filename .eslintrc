{
	"env": {
		"es6": true,
		"browser": true
	},
	"extends": [
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"plugins": [
		"import",
		"prettier"
	],
	"rules": {
		"react/prop-types": 0,
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"no-tabs": 0,
		"indent": [
			4,
			"tab"
		],
		"linebreak-style": 0,
		"global-require": 0,
		"no-underscore-dangle": 0,
		"eslint linebreak-style": [
			0,
			"error",
			"windows"
		],
		"prettier/prettier": [
			"error",
			{
				"singleQuote": true,
				"tabWidth": 4,
				"useTabs": true
			}
		]
	},
	"parser": "babel-eslint"
}
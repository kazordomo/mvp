{
	"extends": [
		"react-app",
		"airbnb",
		"plugin:jsx-a11y/recommended",
		"prettier",
		"prettier/react"
	],
	"plugins": [
		"jsx-a11y",
		"prettier"
	],
	"rules": {
		"semi": 1,
		"react/jsx-props-no-spreading": "off",
		"react/prop-types": 0,
		"react/destructuring-assignment": 0,
		"react/state-in-constructor": 0,
		"indent": [
			"error",
			"tab"
		],
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"prettier/prettier": [
			"error",
			{
				"semi": true
			}
		]
	}
}